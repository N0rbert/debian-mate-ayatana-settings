[Toplevel bottom]
expand=true
orientation=bottom
size=28

[Object briskmenu]
object-type=applet
applet-iid=BriskMenuFactory::BriskMenu
toplevel-id=bottom
position=0
locked=true

[Object showdesktopapplet]
locked=true
position=10
toplevel-id=bottom
applet-iid=WnckletFactory::ShowDesktopApplet
object-type=applet

[Object firefox]
locked=true
position=20
launcher-location=/var/lib/snapd/desktop/applications/firefox_firefox.desktop
toplevel-id=bottom
object-type=launcher
menu-path=applications:/

[Object evolution]
locked=true
position=30
launcher-location=/usr/share/applications/org.gnome.Evolution.desktop
toplevel-id=bottom
object-type=launcher
menu-path=applications:/

[Object window-list]
object-type=applet
applet-iid=WnckletFactory::WindowListApplet
toplevel-id=bottom
position=40
locked=true

[Object drivemountapplet]
object-type=applet
applet-iid=DriveMountAppletFactory::DriveMountApplet
toplevel-id=bottom
position=20
relative-to-edge=end
locked=true

[Object notification-area]
object-type=applet
applet-iid=NotificationAreaAppletFactory::NotificationArea
toplevel-id=bottom
position=10
relative-to-edge=end
locked=true

[Object indicatorappletcomplete]
object-type=applet
applet-iid=IndicatorAppletCompleteFactory::IndicatorAppletComplete
toplevel-id=bottom
position=0
relative-to-edge=end
locked=true
